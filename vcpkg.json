{
  "name": "jarvisto",
  "version-semver": "0.3.12",
  "license": "Apache-2.0",
  "maintainers": [
    "Denys Asauliak <denoming@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    "date",
    "spdlog",
    "nlohmann-json",
    "libsystemd"
  ],
  "features": {
    "network": {
      "description": "Network supporting",
      "dependencies": [
        {
          "name": "boost-asio",
          "features": [
            "ssl"
          ]
        },
        "boost-beast",
        "mosquitto",
        "libsigcpp"
      ]
    },
    "tests": {
      "description": "Tests supporting",
      "dependencies": [
        "gtest"
      ]
    }
  }
}
